{
  "name": "@tinijs/cli",
  "version": "0.0.3",
  "description": "The CLI for the TiniJS framework.",
  "author": "Lam Nhan",
  "homepage": "https://lamnhan.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/public-api.js",
  "types": "src/public-api.d.ts",
  "bin": {
    "tini": "src/bin.js"
  },
  "files": [
    "src",
    "!**/*.ts",
    "**/*.d.ts"
  ],
  "scripts": {
    "compile": "tsc",
    "build": "npm run compile && npm i -g",
    "deploy": "npm run build && npm publish",
    "docs": "ayedocs generate",
    "test": "npm run compile && nyc --cache false mocha test/**/*.js",
    "coverage": "nyc --cache false report --reporter=text-lcov | coveralls",
    "lint": "gts lint",
    "fix": "gts fix"
  },
  "keywords": [
    "node",
    "module",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tinijs/cli.git"
  },
  "bugs": {
    "url": "https://github.com/tinijs/cli/issues"
  },
  "dependencies": {
    "@parcel/packager-raw-url": "^2.8.2",
    "@parcel/reporter-bundle-analyzer": "^2.8.2",
    "@parcel/transformer-sass": "^2.8.2",
    "@parcel/transformer-webmanifest": "^2.8.2",
    "@tinijs/parcel-reporter-build-pwa": "0.0.2",
    "@tinijs/parcel-reporter-copy-public": "0.0.2",
    "@tinijs/parcel-transformer-default": "0.0.2",
    "adm-zip": "^0.5.10",
    "axios": "^1.2.2",
    "chalk": "^5.2.0",
    "change-case": "^4.1.2",
    "commander": "^6.1.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.1.0",
    "inquirer": "^9.1.4",
    "open": "^8.4.0",
    "parcel": "^2.8.2",
    "picomatch": "^2.3.1",
    "recursive-readdir": "^2.2.3",
    "sharp": "^0.31.3",
    "superstatic": "^9.0.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.18",
    "@types/picomatch": "^2.3.0",
    "@types/recursive-readdir": "^2.2.1",
    "gts": "^4.0.0",
    "typescript": "~4.9.4"
  }
}
