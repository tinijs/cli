{
  "name": "@tinijs/cli",
  "version": "0.0.1",
  "description": "The CLI for the TiniJS framework.",
  "author": "Lam Nhan",
  "homepage": "https://lamnhan.com",
  "license": "MIT",
  "main": "src/public-api.js",
  "types": "src/public-api.d.ts",
  "bin": {
    "tini": "src/bin.js"
  },
  "files": [
    "src",
    "!**/*.ts",
    "**/*.d.ts"
  ],
  "scripts": {
    "compile": "tsc",
    "build": "npm run compile && npm i -g",
    "docs": "ayedocs generate",
    "test": "npm run compile && nyc --cache false mocha test/**/*.js",
    "coverage": "nyc --cache false report --reporter=text-lcov | coveralls",
    "lint": "gts lint",
    "fix": "gts fix"
  },
  "keywords": [
    "node",
    "module",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tinijs/cli.git"
  },
  "bugs": {
    "url": "https://github.com/tinijs/cli/issues"
  },
  "dependencies": {
    "@parcel/reporter-bundle-analyzer": "^2.8.2",
    "@parcel/transformer-sass": "^2.8.2",
    "@parcel/transformer-typescript-tsc": "^2.8.2",
    "@tinijs/parcel-transformer-default": "file:../parcel-transformer-default/tinijs-parcel-transformer-default-0.0.1.tgz",
    "adm-zip": "^0.4.16",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "change-case": "^4.1.1",
    "chokidar": "^3.5.3",
    "commander": "^6.1.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "open": "^8.2.0",
    "parcel": "^2.8.2",
    "parcel-reporter-static-files-copy": "^1.4.0",
    "picomatch": "^2.3.1",
    "recursive-readdir": "^2.2.2",
    "sharp": "^0.31.3",
    "superstatic": "^7.1.0"
  },
  "devDependencies": {
    "@lamnhan/ayedocs": "^1.0.7",
    "@types/adm-zip": "^0.4.33",
    "@types/chai": "^4.2.13",
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^7.3.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "@types/picomatch": "^2.3.0",
    "@types/recursive-readdir": "^2.2.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cpy-cli": "^3.1.1",
    "gts": "^3.0.1",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "typescript": "^4.0.3"
  }
}
